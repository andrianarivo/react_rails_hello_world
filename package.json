{
  "name": "react_rails_hello_world",
  "version": "1.0.0",
  "description": "My little rails playground",
  "private": "true",
  "scripts": {
    "lint": "eslint '**/*.{js,jsx}'; stylelint '**/*.{css,scss}'; rubocop",
    "lint:fix": "eslint --fix '**/*.{js,jsx}'; stylelint --fix '**/*.{css,scss}'; rubocop -A",
    "test:coverage": "jest --coverage",
    "test": "jest --coverage --watch",
